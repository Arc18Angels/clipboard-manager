{
  "manifest_version": 2,
  "version": "0.1.3",
  "name": "Clipboard History Manager",
  "description": "A full-featured desktop clipboard manager for your operating system to track what you copy & paste",
  "offline_enabled": true,
  "permissions": [
    "idle",
    "storage",
    "clipboard",
    "unlimitedStorage",
    "notifications"
  ],
  "homepage_url": "https://add0n.com/clipboard-manager.html",
  "app": {
    "background": {
      "scripts": [
        "monitor.js",
        "md5.js",
        "xapian.js",
        "background.js",
        "xapian/xapian.js"
      ]
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "19": "data/icons/19.png",
    "32": "data/icons/32.png",
    "38": "data/icons/38.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png",
    "256": "data/icons/256.png",
    "512": "data/icons/512.png"
  },
  "commands": {
    "open": {
      "description": "Open the manager in a standalone window"
    }
  },
  "options_ui": {
    "page": "data/options/index.html",
    "chrome_style": true
  }
}
